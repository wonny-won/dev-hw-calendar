{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ESNext",
		"moduleResolution": "Node",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./dist",
		"declaration": true,
		"declarationMap": true,
		"jsx": "react"
	},
	"include": ["./src/lib/**/*.tsx", "./src/lib/**/*.ts"]
}
